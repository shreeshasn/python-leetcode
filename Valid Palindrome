class Solution(object):
    def isPalindrome(self, s):
        """
        :type s: str
        :rtype: bool
        """
        st=s.lower()
        st1=''
        for i in st:
            if i.isalnum():
                st1=st1+i
        rev=st1[::-1]
        if rev==st1:
            return True
        else:
            return False
